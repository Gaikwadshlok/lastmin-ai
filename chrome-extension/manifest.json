{
  "manifest_version": 3,
  "name": "Lastmin AI Web Bridge",
  "version": "1.0",
  "description": "Lets Lastmin AI fetch data from the web via this extension.",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "scripting",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon48.png"
  }
}